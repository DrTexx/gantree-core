{
    "metadata": {
        "version": "2.0",
        "project": "my-gantree-project"
    },
    "binary": {
        "filename": "node-template",
        "repository": {
            "url": "https://github.com/substrate-developer-hub/substrate-node-template.git",
            "version": "8b6fe66"
        }
    },
    "defaults": {
        "nodes": {
            "palletOptions": {
                "aura": "$sr25519"
            },
            "binaryOptions": {
                "telemetry": true,
                "loggingFilter": "sync=trace,afg=trace,babe=debug"
            },
            "instance": {
                "sshPublicKey": "<public ssh key>"
            },
            "validator": false
        }
    },
    "nodes": [
        {
            "name": "my-node-1",
            "validator": false,
            "pallet_options": {
                "grandpa": {
                    "address": "$ed25519",
                    "weight": 3
                }
            },
            "instance": {
                "provider": "do",
                "machineType": "s-1vcpu-1gb",
                "zone": "nyc3",
                "sshUser": "root",
                "sshPublicKey": "ssh-rsa  rozifus@calv-subsecval"
            }
        },

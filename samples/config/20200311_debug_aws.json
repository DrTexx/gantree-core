{
    "metadata": {
        "version": "2.0",
        "project": "my-gantree-project"
    },
    "binary": {
        "filename": "node-template",
        "repository": {
            "url": "https://github.com/substrate-developer-hub/substrate-node-template.git",
            "version": "8b6fe66"
        }
    },
    "defaults": {
        "nodes": {
            "palletOptions": {
                "aura": "$sr25519"
            },
            "binaryOptions": {
                "telemetry": true,
                "loggingFilter": "sync=trace,afg=trace,babe=debug"
            },
            "instance": {
                "sshPublicKey": "<public ssh key>"
            },
            "validator": false
        }
    },
    "nodes": [
        {
            "name": "my-node-1",
            "validator": false,
            "pallet_options": {
                "grandpa": {
                    "address": "$ed25519",
                    "weight": 3
                }
            },
            "instance": {
                "provider": "aws",
                "sshUser": "ubuntu",
                "sshPublicKey": "ssh-rsa  rozifus@rama-gandev",
                "machineType": "t3.small",
                "volumeSize": 200,
                "location": "eu-central-1",
                "zone": "eu-central-1",
                "firewall": {
                    "allowed": [
                        {
                            "protocol": "tcp",
                            "from": 22,
                            "to": 22,
                            "cidrIp": "0.0.0.0/0"
                        },
                        {

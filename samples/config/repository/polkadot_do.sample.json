{
    "metadata": {
        "version": "2.0",
        "project": "s-r-do-pol-kus"
    },
    "binary": {
        "chain": false,
        "repository": {
            "url": "https://github.com/paritytech/polkadot",
            "version": "HEAD"
        },
        "filename": "polkadot",
